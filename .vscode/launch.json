    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Server",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\QCS\\manage.py",
            "console": "integratedTerminal",
            "args": [
                "runserver",
                "--noreload"
            ],
            "django": true
        },
        {
            "name": "Create Super User",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\QCS\\manage.py",
            "args": [
                "createsuperuser"
            ],
            "django": true
        },
        {
            "name": "Migrations",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\QCS\\manage.py",
            "args": [
                "makemigrations",
                "Question"
            ],
            "django": true
        },
        {
            "name": "Migrate",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\QCS\\manage.py",
            "args": [
                "migrate"
            ],
            "django": true
        },
    ]
}